{
  "name": "BTG-frontend",
  "version": "0.1.0",
  "description": "React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "private": true,
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "scripts": {
    "clean:out": "rimraf out/*",
    "lint": "eslint \"./**/*.{ts,tsx,css,js,json}\" --quiet",
    "lint:fix": "eslint \"./**/*.{ts,tsx,css,js,json}\" --quiet --fix",
    "start": "serve out -p 8081",
    "start:dev": "next dev -p 8081 ",
    "build": "yarn clean:out && next build && next export",
    "deploy:dev": "sh deploy-dev.sh",
    "deploy:staging": "sh deploy-staging.sh",
    "deploy:prod": "sh deploy-prod.sh",
    "firebase:token:generate": "firebase login:ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx,css,js,json}": [
      "eslint --quiet"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.4",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "lint-staged": ">=10",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.9",
    "@react-google-maps/api": "^2.1.1",
    "@types/express": "^4.17.11",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "cross-fetch": "^3.0.6",
    "date-fns": "^2.17.0",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "firebase": "^8.2.9",
    "firebase-tools": "^9.8.0",
    "moment": "^2.29.1",
    "next": "^10.0.7",
    "query-string": "^6.14.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-number-format": "^4.4.4",
    "react-phone-input-2": "^2.13.9",
    "react-redux": "^7.2.2",
    "react-responsive": "^8.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "serve": "^11.3.2",
    "uuid": "^8.3.2"
  }
}
